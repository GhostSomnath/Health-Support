    <script>
        // Function to toggle dark theme and change logos
        function showTheme() {
            const body = document.body;
            body.classList.toggle('dark');

            // Toggle dark theme on elements with class 'darkable'
            document.querySelectorAll('.darkable').forEach(element => {
                element.classList.toggle('dark');
            });

            // Change navbar logo based on the theme
            const navLogo = document.getElementById('nav-logo');
            if (navLogo) {
                navLogo.src = body.classList.contains('dark')
                    ? '../Images/logo1.png'
                    : '../Images/logo1.png';
            }

            // Change footer logo based on the theme
            const footerLogo = document.getElementById('footerlogo');
            if (footerLogo) {
                footerLogo.src = body.classList.contains('dark')
                    ? '../Images/darkshortlogo.png'
                    : '../Images/logo3-removebg-preview.png';
            }

            // Change the background image based on the theme
            if (body.classList.contains('dark')) {
                body.style.backgroundImage = "url('data:image/png;base64,')";
            } else {
                body.style.backgroundImage = "url('https://images.unsplash.com/photo-1488998427799-e3362cec87c3?w=600&auto=format&fit=crop&q=60&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxzZWFyY2h8MjZ8fGNvbnRhY3QlMjB1c3xlbnwwfHwwfHx8MA%3D%3D')";
            }

            // Change the theme icon
            const themeIcon = document.getElementById('theme-icon');
            themeIcon.classList.toggle('fa-sun', !body.classList.contains('dark'));
            themeIcon.classList.toggle('fa-moon', body.classList.contains('dark'));
        }

        // Add event listener to the button
        document.querySelector('.toggle-theme').addEventListener('click', showTheme);
    </script>










